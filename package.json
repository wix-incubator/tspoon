{
  "private": false,
  "name": "tspoon",
  "version": "1.0.99",
  "description": "AST visitors for Typescript",
  "main": "./dist/src/index.js",
  "jsnext:main": "src/index.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run build:src && npm run build:docs",
    "build:clean": "npm run clean && npm run build",
    "bundle": "webpack -v --display-modules",
    "build:src": "tsc",
    "build:docs": "gulp markdown && gulp typedoc",
    "publish:docs": "gulp publish-docs",
    "pretest": "npm run bundle",
    "test": "npm run test:node",
    "test:browser": "karma start --single-run",
    "test:node": "mocha --reporter mocha-env-reporter ./dist/test",
    "prepublish": "typings install",
    "start": "concurrent -k \"npm run start:tsc\" \"npm run start:webpack\"",
    "start:webpack": "webpack-dev-server --progress --hot --inline --no-colors",
    "start:tsc": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wix/tspoon.git"
  },
  "author": "Gady Guy <gadig@wix.com> (https://github.com/gadiguy)",
  "contributors": [
    "Amir Arad <amira@wix.com> (http://github.com/amir-arad)",
    "Doron Tsur <doront@wix.com> (http://github.com/qballer)",
    "Gady Guy <gadig@wix.com> (https://github.com/gadiguy)"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/wix/tspoon/issues"
  },
  "homepage": "https://github.com/wix/tspoon",
  "devDependencies": {
    "npminject": "1.0.6",
    "chai": "3.5.0",
    "concurrently": "2.2.0",
    "convert-source-map": "1.3.0",
    "event-stream": "3.3.4",
    "github-markdown": "2.2.0",
    "gulp": "3.9.1",
    "gulp-gh-pages": "0.5.4",
    "gulp-markdown": "1.2.0",
    "gulp-util": "3.0.7",
    "json-loader": "0.5.4",
    "karma": "1.1.2",
    "karma-chrome-launcher": "1.0.1",
    "karma-mocha": "1.1.1",
    "mocha": "2.5.3",
    "mocha-env-reporter": "1.0.2",
    "mocha-loader": "0.7.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "source-map-support": "0.4.2",
    "typedoc": "0.3.12",
    "typings": "1.3.2",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "lodash": "4.14.1",
    "magic-string": "0.15.2",
    "source-map": "0.5.6",
    "typescript": "1.8.10"
  }
}
