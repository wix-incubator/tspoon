{
  "private": false,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  },
  "name": "tspoon",
  "version": "1.0.6",
  "description": "see README.md",
  "main": "./dist/src/index.js",
  "jsnext:main": "src/index.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "pretest": "npm run build",
    "test": "npm run test:node",
    "test:browser": "./node_modules/.bin/karma start --single-run",
    "test:node": "./node_modules/.bin/mocha --reporter $npm_config_reporter ./dist/test",
    "build:tsc": "./node_modules/.bin/tsc -declaration",
    "build": "npm run clean && npm run build:tsc",
    "build:src": "npm run build",
    "prestart": "npm run build",
    "start": "./node_modules/.bin/concurrent -k \"npm run start:tsc\" \"npm run start:webpack\"",
    "start:tsc": "./node_modules/.bin/tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix/tspoon"
  },
  "author": "core3@wix",
  "license": "C",
  "bugs": {
    "url": "https://github.com/wix/tspoon/issues"
  },
  "homepage": "https://github.com/wix/tspoon",
  "devDependencies": {
    "chai": "3.2.0",
    "concurrently": "0.1.1",
    "json-loader": "^0.5.1",
    "karma": "0.13.9",
    "karma-chrome-launcher": "0.2.0",
    "karma-mocha": "0.2.0",
    "mocha": "2.2.5",
    "mocha-loader": "0.7.1",
    "mocha-teamcity-reporter": "0.0.1",
    "raw-loader": "0.5.1",
    "source-map-support": "0.3.2",
    "ts-loader": "0.5.3",
    "tsd": "0.6.4",
    "webpack": "1.12.0",
    "webpack-dev-server": "1.10.1"
  },
  "dependencies": {
    "magic-string": "0.6.6",
    "lodash": "3.7.0",
    "typescript": "1.6.2",
    "source-map": "0.4.4"
  }
}
