{
  "private": true,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  },
  "version": "0.1.0",
  "main": "dist/src/index.js",
  "scripts": {
    "build:src": "tsc --declaration --module CommonJS -sourcemap --target ES5 --outDir dist/src src/*.ts",
    "build:test": "tsc --declaration --module CommonJS -sourcemap --target ES5 --outDir dist test/*.ts",
    "build:testkit": "tsc --declaration --module CommonJS -sourcemap --target ES5 --outDir dist/test-kit test-kit/*.ts",
    "build": "rm -rf dist && mkdir dist && npm run build:src && npm run build:testkit && npm run build:test",
    "publish:tc": "./node_modules/.bin/ci",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/mocha --reporter $npm_config_reporter ./dist/test",
    "start": "npm run webtest",
    "webtest": "webpack-dev-server --progress --hot --inline --no-colors",
    "webbuild": "webpack --progress -p --colors"
  },
  "description": "see README.md ",
  "devDependencies": {
	"source-map-support" : "0.3.2",
    "esprima-fb": "git+ssh://git@github.com:wix/esprima-fb.git#feature/class-property-init",
    "chai": "^2.1.2",
    "mocha": "^2.2.1",
    "mocha-loader": "^0.7.1",
    "mocha-teamcity-reporter": "^0.0.1",
    "requirejs": "^2.1.17",
    "source-map": "^0.4.2",
    "vinyl": "^0.4.6",
    "webpack": "^1.7.3",
    "webpack-dev-server": "^1.7.0",
    "sinon": "git://github.com/cjohansen/Sinon.JS#b672042043517b9f84e14ed0fb8265126168778a",
    "sinon-chai": "^2.7.0"
  },
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "lodash": "3.7.0",
    "typescript": "Microsoft/TypeScript#53239d4a8bff80b7ae802a9a0392edd91d1a1295",
    "typorama": "0.0.64"
  },
  "config": {
    "projectLint": "overreact-private"
  },
  "name": "tspoon",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wix/tspoon.git"
  },
  "author": "Mrs. Pepperpot",
  "license": "ISC"
}
